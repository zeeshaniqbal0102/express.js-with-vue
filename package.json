{
  "name": "backoffice",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "dev": "npm run serve --development -- --port 8082",
    "serve": "vue-cli-service serve",
    "dev-build": "vue-cli-service build --mode development",
    "staging-build": "vue-cli-service build --mode staging",
    "build": "vue-cli-service build --mode production",
    "staging-deploy": "vue-cli-service build --mode staging && aws s3 sync ./dist s3://stagingadmin.cellsmart.biz",
    "deploy-production": "vue-cli-service build --mode production && aws s3 sync ./dist s3://admin.cellsmart.biz && npm run invalidate",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E230P8KBF0W245 --paths '/*'",
    "lint": "vue-cli-service lint",
    "cy:fix": "npx cypress install --force",
    "cy:lint": "eslint ./tests/**",
    "cy:open": "cypress open --env configFile=qa",
    "cy:run": "cypress run --env configFile=qa",
    "cy:run:chrome": "cypress run --env configFile=qa --headless --browser chrome",
    "ci": "run-s dev-build ci:cy-run",
    "ci:cy-run": "start-server-and-test ci:start-server 8082 cy:run:chrome",
    "ci:start-server": "serve -s ./dist -p 8082",
    "ci:dev:build": "run-s lang listUpdate dev-build",
    "ci:staging:build": "run-s lang listUpdate staging-build",
    "ci:production:build": "run-s lang listUpdate build",
    "lang": "npm install cellsmart-lang",
    "listUpdate": "npx browserslist@latest --update-db"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "axios-extensions": "^3.1.3",
    "bwip-js": "^1.7.3",
    "cellsmart-lang": "git+https://bitbucket.org/msaqib1980/csp4-language.git",
    "chart.js": "^2.9.4",
    "compact-timezone-list": "^1.0.6",
    "decimal.js": "^10.2.1",
    "html2canvas": "^1.0.0-rc.7",
    "lodash": "^4.17.20",
    "moment-timezone": "^0.5.31",
    "pdfmake": "^0.1.56",
    "pdfvuer": "~1.6.1",
    "portal-vue": "^2.1.5",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-adblock": "0.0.3",
    "vue-analytics": "^5.16.4",
    "vue-axios": "^2.1.4",
    "vue-ctk-date-time-picker": "^2.1.1",
    "vue-currency-filter": "^3.4.2",
    "vue-google-charts": "^0.3.2",
    "vue-google-oauth2": "^1.5.4",
    "vue-i18n": "^8.18.1",
    "vue-image-upload-resize": "^2.0.4",
    "vue-js-toggle-button": "^1.3.2",
    "vue-moment": "^4.1.0",
    "vue-progressbar": "^0.7.5",
    "vue-router": "^3.0.6",
    "vue-stripe-elements-plus": "^0.2.9",
    "vue-sweetalert2": "^1.6.4",
    "vue-toasted": "^1.1.26",
    "vue-tour": "^1.1.0",
    "vuelidate": "^0.7.4",
    "vuetify": "^1.5.11",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.4.10",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@types/faker": "^5.1.4",
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-plugin-pwa": "^3.7.0",
    "@vue/cli-service": "^4.5.8",
    "@vue/eslint-config-airbnb": "~4.0.1",
    "@vue/eslint-config-typescript": "~4.0.0",
    "@vue/test-utils": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "cypress": "^5.6.0",
    "eslint": "^5.16.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-vue": "~5.2.3",
    "faker": "^5.1.0",
    "fs-extra": "^9.0.1",
    "lint-staged": "^8.1.7",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.2",
    "start-server-and-test": "^1.11.5",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.1",
    "ts-loader": "^8.0.7",
    "typescript": "~3.5.3",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.4.4",
    "webpack": "~4.44.2"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "semi": 0,
      "max-len": 0,
      "no-plusplus": 0,
      "no-param-reassign": 0,
      "no-console": 0,
      "consistent-return": 0,
      "import/extensions": 0,
      "func-names": 0,
      "no-unreachable": 0,
      "prefer-destructuring": 0,
      "vue/no-unused-components": 0,
      "default-case": 0,
      "radix": 0,
      "import/no-extraneous-dependencies": 0,
      "no-empty": 0,
      "no-shadow": 0,
      "vue/return-in-computed-property": 0,
      "no-restricted-globals": 0,
      "no-sequences": 0,
      "no-unused-expressions": 0,
      "prefer-promise-reject-errors": 0,
      "vue/valid-v-for": 0,
      "array-callback-return": 0,
      "no-return-assign": 0,
      "vue/no-async-in-computed-properties": 0,
      "vue/valid-v-on": 0,
      "eqeqeq": 0,
      "vue/no-parsing-error": 0,
      "vue/no-use-v-if-with-v-for": 0,
      "vue/valid-v-else": 0,
      "no-alert": 0,
      "no-fallthrough": 0,
      "vue/valid-template-root": 0,
      "vue/no-dupe-keys": 0,
      "no-dupe-keys": 0,
      "vue/valid-v-bind": 0,
      "no-bitwise": 0,
      "vue/no-unused-vars": 0,
      "no-underscore-dangle": 0,
      "no-case-declarations": 0,
      "import/prefer-default-export": 0,
      "no-nested-ternary": 0,
      "implicit-arrow-linebreak": 0,
      "no-trailing-spaces": 0,
      "nonblock-statement-body-position": 0,
      "curly": 0,
      "object-curly-newline": 0,
      "arrow-parens": 0,
      "operator-linebreak": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
